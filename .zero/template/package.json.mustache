{
  "name": "{{name}}",
  "fullName": "{{fullName}}",
  "version": "0.0.0",
  "description": "create your js lib from zero.",
  "main": "dist/{{fullName}}.cjs.js",
  "module": "dist/{{fullName}}.es.js",
  "browser": "dist/{{fullName}}.umd.js",
  "types": "typings/index.d.ts",
  "typings": "typings/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build-all": "rmdir-cli dist && rmdir-cli temp  && npm run build && npm run build-bundle",
    "build-ts": "tsc src/index.ts -d -m commonjs --outDir dist/ts ",
    "build-bundle": "webpack --verbose --mode=production",
    "build-and-publish": "npm run build-all && npm run doc && npm run publish",
    "doc": "jsdoc2md dist/{{fullName}}.es.js > doc/{{fullName}}.md && type README-HEAD.MD > README.MD && type doc\\{{fullName}}.md >> README.MD",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "publish": "npm publish --registry=https://registry.npmjs.org/ --disturl=/",
    "zero-init": "node ./.zero/zero-init.js"
  },
  "author": "nullice",
  "license": "MIT",
  "repository": "",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.56",
    "@babel/runtime": "^7.0.0-beta.56"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-external-helpers": "^7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.56",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.7",
    "babel-core": "^7.0.0-beta.3",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "jest": "^23.4.2",
    "mustache": "2.3.1",
    "prettier": "1.14.0",
    "rmdir-cli": "^2.0.6",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-cpy": "1.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-uglify": "4.0.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "coveragePathIgnorePatterns":["/node_modules/", "/dist/"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
       "@/(.*)": "<rootDir>/src/$1"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "files": [
    "dist",
    "doc",
    "LICENSE.md",
    "README.md",
    "typings"
  ]
}
